{#
/**
 * @file
 * Include toolbar.html.twig template from Gin.
 */
#}
{% include '@gin/navigation/toolbar--gin.html.twig' ignore missing %}
{% if toolbar_variant != 'classic' %}
<div class="region region-meta region-meta--frontend">
  <div class="gin-layout-container">
    <div class="breadcrumb-wrapper">
      <div class="region region-breadcrumb">
      {% if route_name == 'entity.node.canonical' %}
        <a class="gin-back-to-admin" href="/node/{{ node_id }}/edit">{{ 'Edit'|t }} {{ node_title }}</a>
      {% else %}
        <a class="gin-back-to-admin" href="/admin/content">{{ 'Back to Administration'|t }}</a>
      {% endif %}
      </div>
    </div>
    {% include '@gin/navigation/toolbar--gin--secondary.html.twig' ignore missing %}
  </div>
</div>
{% endif %}
